{
	"name": "incrgraph",
	"version": "1.0.0",
	"scripts": {
		"install": "npm run install:frontend && npm run install:backend",
		"install:frontend": "cd frontend && npm install",
		"install:backend": "cd backend && npm install",
		"build": "npm run build:frontend && npm run build:backend",
		"build:frontend": "cd frontend && npm run build",
		"build:backend": "cd backend && npm run build",
		"start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
		"start:frontend": "cd frontend && npm run dev",
		"start:backend": "cd backend && npm run dev",
		"prod": "npm run build && concurrently \"serve -s frontend/dist\" \"cd backend && npm run start\""
	},
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MaxMB15/MSc-SE-Master-Project.git"
      },
    "author": "Max Boksem",
    "license": "GPL-3.0",
    "description": "Root of IncrGraph",
    "bugs": {
        "url": "https://github.com/MaxMB15/MSc-SE-Master-Project/issues"
      },
      "homepage": "https://github.com/MaxMB15/MSc-SE-Master-Project#readme",
    
	"dependencies": {
		"concurrently": "^6.0.0"
	},
	"devDependencies": {
		"eslint-config-prettier": "^9.1.0"
	}
}
